	ARGS() [<comma> <star> ARG()] | <star> ARG()
}

/*
 * @Production
 * THEN 	: T | then | T then //"then" and "do" can go on next line
 */
void THEN() : 
{}
{
	<T>
	| <keyword_then>
	| [<eps18>]<T> <keyword_then> //"then" and "do" can go on next line // hook [<eps>]
}

/*
