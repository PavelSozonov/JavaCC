/*
 * @Production
 * OP_ASGN	: += | -= | *= | /= | %= | **=
 * 		| &= | |= | ^= | <<= | >>=
 * 		| &&= | ||=
 */
void OP_ASGN() : 
{}
{
	<plusequal>
	| <minusequal>
	| <starequal>
	| <slashequal>
	| <percentequal>
	| <powerequal>
	| <amperequal>
	| <pipeequal>
	| <caretequal>
	| <lshiftequal>
	| <rshiftequal>
	| <andequal>
	| <orequal>
}

/*
 * @Production
 * SYMBOL	: :FNAME | :VARNAME
 */
void SYMBOL() : 
{}
{
	 <colon>FNAME()
	 | <colon>VARNAME()
}

/*
 * @Production
 * FNAME	: IDENTIFIER | .. | "|" | ^ | & | <=> | == | === | =~
 * 		| > | >= | < | <= | + | - | * | / | % | **
 * 		| << | >> |  Мѓ | +@ | -@ | [] | []=
 */
void FNAME() : 
{}
{
	IDENTIFIER()
	| <dot2>
	| <pipe>
	| <caret>
	| <amper>
	| <lessequalmore>
	| <equal2>
	| <equal3>
	| <equaltilde>
	| <more>
	| <moreorequal>
	| <less>
	| <lessorequal>
	| <plus>
	| <minus>
	| <star>
	| <slash>
	| <percent>
	| <power>
	| <lshift>
	| <rshift>
	| <tilde>
	| <plusat>
	| <minusat>
	| <sqbrackets>
	| <sqbracketsequal>
}

