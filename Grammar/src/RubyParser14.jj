void MLHS_ITEM() : 
{}
{
	LHS()
	| [<eps20>]<lparen> MLHS() <rparen> // hook [<eps>]
}

/*
 * @Production
 * LHS 	: VARIABLE
 * 		| PRIMARY "[" [ARGS] "]"
 * 		| PRIMARY.IDENTIFIE
 */
void LHS() : 
{}
{
	 	VARIABLE()
		| PRIMARY() <lsqbracket> [ARGS()] <rsqbracket>
		| [<eps21>]PRIMARY() <dot> IDENTIFIER() // hook [<eps>]
}

/*
 * @Production
 * MRHS 	: ARGS [, * ARG] |  * ARG
 */
void MRHS() : 
{}
