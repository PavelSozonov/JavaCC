void DO() : 
{}
{
	<T>
	| <keyword_do>
	| [<eps19>]<T> <keyword_do> // hook [<eps>] 
}

/*
 * @Production
 * BLOCK_VAR : LHS | MLHS
 */
void BLOCK_VAR() : 
{}
{
	 LHS() | MLHS()
}

/*
 * @Production
 * MLHS 	: MLHS_ITEM , [MLHS_ITEM (, MLHS_ITEM)*] [* [LHS]]
 * 			| * LHS
 */
void MLHS() : 
{}
{
	MLHS_ITEM() <comma> [MLHS_ITEM() (<comma> MLHS_ITEM())*] [<star> [LHS()]]
	| <star> LHS()
}

