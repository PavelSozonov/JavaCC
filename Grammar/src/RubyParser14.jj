{
	<plusequal>
	| <minusequal>
	| <starequal>
	| <slashequal>
	| <percentequal>
	| <powerequal>
	| <amperequal>
	| <pipeequal>
	| <caretequal>
	| <lshiftequal>
	| <rshiftequal>
	| <andequal>
	| <orequal>
}

/*
 * @Production
 * SYMBOL	: :FNAME | :VARNAME
 */
void SYMBOL() : 
{}
{
	 <colon>FNAME()
	 | [<eps24>]<colon>VARNAME() // hook [<eps>]
}

/*
 * @Production
 * FNAME	: IDENTIFIER | .. | "|" | ^ | & | <=> | == | === | =~
 * 		| > | >= | < | <= | + | - | * | / | % | **
 * 		| << | >> |  ̃ | +@ | -@ | [] | []=
 */
void FNAME() : 
{}
{
	IDENTIFIER()
	| <dot2>
	| <pipe>
	| <caret>
	| <amper>
	| <lessequalmore>
	| <equal2>
	| <equal3>
	| <equaltilde>
	| <more>
	| <moreorequal>
	| <less>
	| <lessorequal>
	| <plus>
	| <minus>
